<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Cellular_Automata_Game_of_Life</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>CellularAutomata.GameOfLife</PackageId>
    <Version>1</Version>
    <Authors>Tim Hanewich</Authors>
    <Product>CellularAutomata.GameOfLife</Product>
    <Description>A recreation of John Conway's "Game of Life".  Uses arrays of boolean values to simulate cells.  Also includes a cloud compute module for computing the next frame in the cloud instead of on the local device to save cycles.</Description>
    <Copyright>Tim Hanewich</Copyright>
    <PackageIcon>GliderIcon.png</PackageIcon>
    <AssemblyVersion>0.0.0.0</AssemblyVersion>
    <FileVersion>0.0.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="GliderIcon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
